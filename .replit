modules = ["c-clang14", "cpp-clang14", "ruby-3.2"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["flutter"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flutter Web Server"

[[workflows.workflow]]
name = "Flutter Web Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter run -d web-server --web-port=5000 --web-hostname=0.0.0.0"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

